{
  "name": "@mystikonetwork/node",
  "version": "0.1.0-0.1.0-alpha.1.0",
  "description": "mystiko library in Node.js",
  "main": "build/cjs/index.js",
  "module": "build/esm/index.js",
  "types": "build/cjs/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/mystikonetwork/mystiko-core",
    "directory": "packages/mystiko-node"
  },
  "author": {
    "name": "Mystiko Network"
  },
  "keywords": [
    "mystiko",
    "web3",
    "ethereum",
    "cryptocurrency"
  ],
  "files": [
    "build"
  ],
  "license": "MIT",
  "scripts": {
    "build:cjs": "tsc -p ./tsconfig.json",
    "build:esm": "tsc -p ./tsconfig.esm.json",
    "build": "rm -rf build && yarn build:cjs && yarn build:esm",
    "test": "jest --forceExit --coverage",
    "lint": "eslint --ext .js,.ts .",
    "prettier:check": "prettier --check \"./**/*.{js,ts}\" --config .prettierrc",
    "prettier:fix": "prettier --write \"./**/*.{js,ts}\" --config .prettierrc",
    "prettier": "yarn prettier:fix"
  },
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.1",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.21",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "eslint": "^8.6.0",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.4.7",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "@mystikonetwork/core": "^0.1.0-0.1.0-alpha.1.0",
    "@mystikonetwork/protocol": "^0.1.0-0.1.0-alpha.1.0",
    "chalk": "^4.1.2",
    "loglevel-plugin-prefix": "^0.8.4",
    "pouchdb-adapter-memory": "^7.2.2",
    "pouchdb-adapter-node-websql": "^7.0.0",
