{
  "name": "mystiko",
  "version": "0.1.0",
  "description": "Core Protocol of Mystiko.Network",
  "private": true,
  "module": "src/index.js",
  "repository": "https://github.com/mystikonetwork/mystiko-core",
  "author": "Fitch",
  "license": "MIT",
  "scripts": {
    "build:circuit": "./scripts/buildCircuit.sh",
    "build:contract": "mkdir -p build && npx truffle compile",
    "build:js": "rm -rf build/js/* && webpack -c webpack.config.js",
    "build": "yarn lint && yarn build:contract && yarn build:js && yarn jsdoc",
    "migrate": "yarn migrate:dev",
    "migrate:dev": "npx truffle migrate --network development --reset",
    "migrate:ropsten": "npx truffle migrate --network ropsten",
    "migrate:bsctestnet": "npx truffle migrate --network bsctestnet",
    "prettier:check": "prettier --check \"./**/*.{js,sol}\" --config .prettierrc",
    "prettier:fix": "prettier --write \"./**/*.{js,sol}\" --config .prettierrc",
    "prettier": "yarn prettier:fix",
    "lint": "yarn prettier:check && yarn lint:js && yarn lint:sol",
    "lint:js": "eslint --ext .js .",
    "lint:sol": "npx solhint contracts/**/*.sol",
    "test": "yarn test:js && yarn test:contract",
    "test:js": "jest --forceExit",
    "test:contract": "npx truffle test",
    "jsdoc": "jsdoc -c jsdoc.json"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.8",
    "@babel/core": "^7.16.7",
    "@babel/eslint-parser": "^7.16.5",
    "@babel/node": "^7.16.8",
    "@babel/plugin-transform-runtime": "^7.16.8",
    "@babel/preset-env": "^7.16.7",
    "@babel/register": "^7.16.8",
    "@openzeppelin/contracts": "^3.4.2",
    "@truffle/contract": "^4.4.2",
    "@truffle/hdwallet-provider": "^2.0.0",
    "assert-browserify": "^2.0.0",
    "babel-jest": "^27.4.6",
    "babel-loader": "^8.2.3",
    "circomlib": "^2.0.2",
    "crypto-browserify": "^3.12.0",
    "dotenv": "^10.0.0",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-truffle": "^0.3.1",
    "ganache-cli": "^6.12.2",
    "https-browserify": "^1.0.0",
    "jest": "^27.4.7",
    "jsdoc": "^3.6.7",
    "node-fetch": "^3.2.0",
    "node-loader": "^2.0.0",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "prettier": "^2.5.1",
    "prettier-plugin-solidity": "^1.0.0-beta.19",
    "process": "^0.11.10",
    "safe-buffer": "^5.2.1",
    "solhint": "^3.3.6",
    "solhint-plugin-prettier": "^0.0.5",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "terser-webpack-plugin": "^5.3.0",
    "truffle": "^5.4.27",
    "truffle-plugin-verify": "^0.5.20",
    "url": "^0.11.0",
    "util": "^0.12.4",
    "webpack": "^5.66.0",
    "webpack-cli": "^4.9.1",
    "webpack-node-externals": "^3.0.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.16.7",
    "@metamask/detect-provider": "^1.2.0",
    "big-integer": "^1.6.51",
    "blake-hash": "^2.0.0",
    "bn.js": "^5.2.0",
    "bs58": "^4.0.1",
    "chalk": "^4.1.2",
    "circomlibjs": "^0.0.8",
    "crypto-js": "^4.1.1",
    "eccrypto": "^1.1.6",
    "ethereumjs-wallet": "^1.0.2",
    "ethers": "^5.5.3",
    "fastfile": "^0.0.19",
    "ffjavascript": "^0.2.48",
    "loglevel": "^1.8.0",
    "loglevel-plugin-prefix": "^0.8.4",
    "lokijs": "^1.5.12",
    "randombytes": "^2.1.0",
    "snarkjs": "^0.4.12"
  }
}
