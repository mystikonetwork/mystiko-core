    "bs58": "^4.0.1",
    "circomlib": "^0.5.5",
    "crypto-js": "^4.1.1",
    "eciesjs": "^0.3.12",
    "ethereumjs-wallet": "^1.0.2",
    "fixed-merkle-tree": "^0.6.0",
    "lokijs": "^1.5.12",
    "web3": "^1.6.1"
  }
