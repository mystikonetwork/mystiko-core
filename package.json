{
  "name": "mystiko",
  "version": "0.1.0",
  "description": "Core Protocol of Mystiko.Network",
  "main": "build/js/cjs/index.js",
  "module": "build/js/esm/index.js",
  "repository": "https://github.com/mystikonetwork/mystiko-core",
  "author": "Fitch",
  "license": "MIT",
  "scripts": {
    "build:circuit": "./scripts/buildCircuit.sh",
    "build:contract": "npx truffle compile",
    "build:js": "rollup -c rollup.config.js",
    "build": "yarn lint && yarn build:contract && yarn build:js",
    "migrate": "yarn migrate:dev",
    "migrate:dev": "npx truffle migrate --network development --reset",
    "migrate:ropsten": "npx truffle migrate --network ropsten --reset",
    "prettier:check": "prettier --check \"./**/*.{js,sol}\" --config .prettierrc",
    "prettier:fix": "prettier --write \"./**/*.{js,sol}\" --config .prettierrc",
    "lint": "yarn prettier:fix && yarn lint:js && yarn lint:solidity",
    "lint:js": "eslint --ext .js .",
    "lint:solidity": "npx solhint contracts/**/*.sol",
    "test": "yarn test:js && yarn test:contract",
    "test:js": "jest",
    "test:contract": "npx truffle test"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.8",
    "@babel/core": "^7.16.7",
    "@babel/eslint-parser": "^7.16.5",
    "@babel/preset-env": "^7.16.7",
    "@babel/register": "^7.16.8",
    "@openzeppelin/contracts": "^3.4.2",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@truffle/contract": "^4.4.2",
    "@truffle/hdwallet-provider": "^2.0.0",
    "babel-jest": "^27.4.6",
    "circom": "^0.5.46",
    "dotenv": "^10.0.0",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-truffle": "^0.3.1",
    "ganache-cli": "^6.12.2",
    "jest": "^27.4.7",
    "prettier": "^2.5.1",
    "prettier-plugin-solidity": "^1.0.0-beta.19",
    "rollup": "^2.63.0",
    "rollup-plugin-terser": "^7.0.2",
    "snarkjs": "^0.4.10",
    "solhint": "^3.3.6",
    "solhint-plugin-prettier": "^0.0.5",
    "truffle": "^5.4.27"
  },
  "dependencies": {
    "blake-hash": "^2.0.0",
    "bn.js": "^5.2.0",
    "browser-or-node": "^2.0.0",
    "bs58": "^4.0.1",
    "circomlib": "^0.5.5",
    "crypto-js": "^4.1.1",
    "eciesjs": "^0.3.12",
    "ethereumjs-wallet": "^1.0.2",
    "ethers": "^5.5.3",
    "fastfile": "^0.0.19",
    "ffjavascript": "^0.2.46",
    "fixed-merkle-tree": "^0.6.0",
    "lokijs": "^1.5.12",
    "randombytes": "^2.1.0"
  }
}
