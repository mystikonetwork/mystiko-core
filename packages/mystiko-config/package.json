{
  "name": "@mystiko/config",
  "version": "0.1.1-alpha.0",
  "description": "configuration of mystiko-core",
  "main": "build/cjs/index.js",
  "module": "build/esm/index.js",
  "types": "build/cjs/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/mystikonetwork/mystiko-core",
    "directory": "packages/mystiko-config"
  },
  "author": {
    "name": "Mystiko Network"
  },
  "keywords": [
    "mystiko",
    "web3",
    "ethereum",
    "cryptocurrency"
  ],
  "files": [
    "build"
  ],
  "license": "MIT",
  "private": true,
  "scripts": {
    "build:cjs": "tsc -p ./tsconfig.json",
    "build:esm": "tsc -p ./tsconfig.esm.json",
    "build": "rm -rf build && yarn build:cjs && yarn build:esm",
    "test": "jest --ci --json --coverage --testLocationInResults --outputFile=coverage/report.json",
    "lint": "eslint --ext .js,.ts .",
    "prettier:check": "prettier --check \"./**/*.{js,ts}\" --config .prettierrc",
    "prettier:fix": "prettier --write \"./**/*.{js,ts}\" --config .prettierrc",
    "prettier": "yarn prettier:fix"
  },
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.1",
    "@types/bn.js": "^5.1.0",
    "@types/jest": "^27.4.0",
    "@types/pako": "^1.0.3",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "dts-gen": "^0.6.0",
    "eslint": "^8.6.0",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.4.7",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.3",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "@babel/runtime": "^7.16.7",
    "@mystiko/utils": "^0.1.1-alpha.0",
    "ethers": "^5.5.3"
  }
}
