{
  "name": "@mystiko/core",
  "version": "0.1.0",
  "description": "Core Protocol of Mystiko.Network",
  "main": "src/index.ts",
  "repository": "https://github.com/mystikonetwork/mystiko-core",
  "author": "Fitch",
  "license": "MIT",
  "scripts": {
    "build:circuit": "./scripts/buildCircuit.sh",
    "build:contract": "npx truffle compile",
    "build": "yarn lint && yarn build:circuit && yarn build:contract",
    "migrate": "yarn migrate:dev",
    "migrate:dev": "npx truffle migrate --network development --reset",
    "eslint": "eslint --ext .js .",
    "prettier:check": "prettier --check \"./**/*.js\" --config .prettierrc",
    "prettier:fix": "prettier --write \"./**/*.js\" --config .prettierrc",
    "lint": "yarn eslint && yarn prettier:check",
    "test": "yarn test:js && yarn test:contract",
    "test:js": "jest",
    "test:contract": "npx truffle test"
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@babel/eslint-parser": "^7.16.5",
    "@babel/preset-env": "^7.16.7",
    "@openzeppelin/contracts": "^4.4.1",
    "@truffle/contract": "^4.4.2",
    "@truffle/hdwallet-provider": "^2.0.0",
    "babel-jest": "^27.4.6",
    "circom": "^0.5.46",
    "dotenv": "^10.0.0",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "ganache-cli": "^6.12.2",
    "jest": "^27.4.7",
    "prettier": "^2.5.1",
    "snarkjs": "^0.4.10",
    "truffle": "^5.4.27"
  },
