    "@types/bn.js": "^5.1.0",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.21",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "command-exists": "^1.2.9",
